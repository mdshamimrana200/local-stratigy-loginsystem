{
  "view": {
    "defaultEngine": "ejs",
    "ext": ".ejs",
    "name": "index",
    "root": "/var/task/views"
  },
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
   
    {
      "src": "/",
      "dest": "/var/task/views/index.ejs"
    }
  ]
}
