{
  "view": {
    "defaultEngine": "ejs",
    "ext": ".ejs",
    "name": "index",
    "root": "/var/task/views"
  },
  "version": 2,
  "builds": [
    {
      "src": "./index.js",
      "use": "@vercel/node"
    }
  ],

  "routes": [
    {
      "src": "/(.*)",
      "dest": "/"
    },
    { "src": "/app.js", "dest": "/var/task/app.js" },
    { "src": "/index.js", "dest": "/var/task/index.js" },
    { "src": "/login", "dest": "/var/task/views/login.ejs" }
  ]
}
